html {
    position: relative;
    min-height: 100%;
}

html, body {
    height: 100%;
    font-size: 16px;
    line-height: 1;
    color: #16181a
    background: #fff
    position: relative
}

img,
embed,
object,
video {
  max-width: 100%;
}

.clearfix {
  &:after {
    content: "";
    display: table;
    clear: both;
  }
}

.container {
  position: absolute;
  left: 0;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}